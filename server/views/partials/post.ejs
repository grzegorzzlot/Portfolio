<div>
    <h2>Post</h2> <a href="add-post" class="btn btn-primary">Add new Post</a>
</div>
<table>
    <thead>
     
        <tr>
        <th scope="col">Title</th>
        <th scope="col">Content</th>
        <th scope="col">Thumbnail</th>
        </tr>
    </thead>
    <tbody>        
        <% data.map((i)=>{%>
        <tr>
            <td><%= i.title%></td>
            <td><%= i.content%></td>
            <td><img class="img-thumbnail" src="<%= i.thumbnail%>" alt="obrazek" /></td>
        </tr>
        <%})%>                    
    </tbody>
</table>
<script language="javascript" src="/javascripts/post.js"></script>